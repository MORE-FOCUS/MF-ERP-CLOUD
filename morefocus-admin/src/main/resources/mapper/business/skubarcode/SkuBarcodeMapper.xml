<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.morefocus.admin.module.business.barcode.mapper.SkuBarcodeMapper">

    <select id="queryBarcodeBySpuId" resultType="cn.morefocus.admin.module.business.barcode.domain.vo.SkuBarcodeVO">
        select t_sku.*
        from t_sku t_sku
        where t_sku.is_deleted = false
          and t_sku.spu_id = #{spuId}
    </select>
</mapper>